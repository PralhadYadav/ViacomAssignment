  "extends": "./tsconfig.base.json",
  "composite": true,
  "enableIvy": false,
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "composite": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "declaration": true
